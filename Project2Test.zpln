\")\n      //.enableHiveSupport()\n      .config(\"spark.master\", \"local\")\n      .config(\"spark.eventLog.enabled\", false)\n      .getOrCreate()\n\n    // Please remember to update hdfs location path\n    val df = spark.read.format(\"csv\").option(\"header\", \"true\").load(\"hdfs://localhost:9000/user/akiem/time_series_covid_19_deaths_US.csv\")\n\n\n    //For Jul 1, 2020 - Jul 31, 2020\n    //Setup variables for framing a month by index\n    val URBANPOPCRITERIA = 50000\n    val header = List(\"Admin2\", \"Province_State\", \"Population\", \"UID\")\n\n    def findDeadliestCity(byMonth: Int, andYear: Int): Unit = {\n    var daysInMonth = collection.mutable.Map( 1 -> 31, 2 -> 28, 3 -> 31,\n                                                4 -> 30, 5 -> 31, 6 -> 30,\n                                                7 -> 31, 8 -> 31, 9 -> 30,\n                                                10 -> 31, 11 -> 30, 12 -> 31)\n        if (andYear == 2020) {\n            // daysInMonth set to to ten because data set starts on the 22nd of Jan to the 31st of Jan\n            daysInMonth(1) = 10; daysInMonth(2) = 29\n        }\n\n\n        var firstEntryIndex = 12\n        var totalDays = 0\n\n\n        for(i <- 1 to byMonth){ totalDays += daysInMonth(i) }\n        if(andYear == 2021) {totalDays += 345}\n\n        \n        val startDayIndex = firstEntryIndex + totalDays - daysInMonth(byMonth) \n        val endDayIndex = firstEntryIndex + totalDays\n\n\n        val testList = header ++ (df.columns.slice(startDayIndex,endDayIndex).toList)\n        val startDay = 4 // per new testListDF columns with added header columns\n        val endDay = daysInMonth(byMonth) + startDay - 1\n\n        val testMthDF = df.select(testList.map(m=>col(m)):_*)\n        val test = testMthDF.withColumn(\"Total\", ((col(testMthDF.columns((endDay))) - col(testMthDF.columns(startDay)))))\n\n        // Top 5 Most Deadly Cities from Urban Areas\n        val testByUrban = test.select(\"*\")\n            .where(col(\"Population\").>=(URBANPOPCRITERIA))\n            //.filter(col(\"Province_State\") === \"Maryland\")\n            .sort(desc(\"Total\")).show(15)\n\n        // Top 5 Most Deadly Cities from Non-Urban Areas\n        val testByNonUrban = test.select(\"*\")\n            .where(col(\"Population\").<(URBANPOPCRITERIA))\n            .where(col(\"Population\").>(0))\n            .sort(desc(\"Total\")).show(15)\n\n    val testGraph = test.withColumnRenamed(\"Province_State\", \"State\").groupBy(\"State\").agg((sum(\"Total\")).as(\"Totals by State\")).sort(desc(\"Totals by State\"))\n    testGraph.show(5)\n    // z.show(testGraph)\n\n    }\n    for(i <- 1 to 4){\n        findDeadliestCity(i, 2021)\n    }\n\n    spark.stop()\n\n  }\n}\ntestData.main(Array(\"\"))\n\n ","user":"bobby","dateUpdated":"2022-07-21T16:31:06-0400","progress":0,"config":{"colWidth":12,"fontSize":9,"results":{},"enabled":true,"editorSetting":{}},"settings":{"params":{},"forms":{}},"results":{"code":"SUCCESS","msg":[{"type":"TEXT","data":"+-----------+--------------+----------+--------+------+------+------+------+------+------+------+------+------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+------+\n|     Admin2|Province_State|Population|     UID|1/1/21|1/2/21|1/3/21|1/4/21|1/5/21|1/6/21|1/7/21|1/8/21|1/9/21|1/10/21|1/11/21|1/12/21|1/13/21|1/14/21|1/15/21|1/16/21|1/17/21|1/18/21|1/19/21|1/20/21|1/21/21|1/22/21|1/23/21|1/24/21|1/25/21|1/26/21|1/27/21|1/28/21|1/29/21|1/30/21|1/31/21| Total|\n+-----------+--------------+----------+--------+------+------+------+------+------+------+------+------+------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+-------+------+\n|Los Angeles|    California|  10039107|84006037| 10552| 10688| 10773| 10852| 11089| 